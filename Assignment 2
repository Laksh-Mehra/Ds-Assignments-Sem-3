(Q4 b)

#include<iostream> 
using namespace std; 
int main(){ 
    string a="abcde"; 
    int i=0; 
    int j=a.size()-1; 
    while(i<j){ 
        swap(a[i],a[j]); 
        i++; 
        j--; 
    }  
    cout<<a; 
} 

(Q4 c)

#include<iostream> 
using namespace std; 
bool vowel(char c){ 
    return (c=='a' || c=='e' || c=='i' || c=='o' || c=='u'); 
} 
int main(){ 
    string a="aevuisofdbc"; 
    string ans=""; 
    for(int i=0;i<a.size();i++){ 
        if(!vowel(a[i])) ans+=a[i]; 
    } 
    cout<<ans; 
} 
