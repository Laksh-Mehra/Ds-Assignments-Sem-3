(Q5 a)

#include<iostream> 
using namespace std; 
int main(){ 
    int arr[3][3]={0}; 
    for(int i=0;i<3;i++){ 
        for(int j=0;j<3;j++){ 
            if(i==j) cin>>arr[i][j]; 
        }
    } 
    for(int i=0;i<3;i++){ 
        for(int j=0;j<3;j++){ 
            cout<<arr[i][j]<<" "; 
        } 
        cout<<endl;
    }
}

(Q5 b)

#include<iostream> 
using namespace std; 
int main() { 
    int arr[3][3] = {0}; 
    for (int i = 0; i < 3; i++) { 
        for (int j = 0; j < 3; j++) { 
            if (i == j || i == j - 1 || i == j + 1) { 
                cin >> arr[i][j]; 
            }
        }
    } 
    for (int i = 0; i < 3; i++) { 
        for (int j = 0; j < 3; j++) { 
            cout << arr[i][j] << " "; 
        } 
        cout << endl;
    } 
    return 0; 
} 

